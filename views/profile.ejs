<h1>
    <%= profile.name %>
</h1>
<p>
    <img src="<%= profile.photo %>" alt="<%= profile.name %> photo">
</p>
<br>
<p>
<h3>Interests</h3>
<ul>
    <% for (let i=0; i < profile.interests.length; i++) { %>
        <li>
            <%= profile.interests[i] %>
        </li>
        <% } %>
</ul>
</p>